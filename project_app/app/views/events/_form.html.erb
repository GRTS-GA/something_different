
    <%= form_for(event) do |f|%>
        <%= f.label :name %><br>
        <%= f.text_field :name %><br>
       <br>
        <%= f.label :type %><br>
        <%= f.text_field :type %><br>
        <br>
        <%= f.label :category %><br>
        <%= f.text_field :category %><br>
        <br>
        <%= f.label :event_date %><br>
        <%= f.text_field :event_date %><br>
        <br>
        <%= f.label :image_url %><br>
        <%= f.text_field :image_url %><br>
        <br>
        <%= f.label :event_url %><br>
        <%= f.text_field :event_url %><br>
        <br>
        <%= f.submit "Submit" %>
    <% end %>

